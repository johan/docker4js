{
  "name": "2-1-dev-vs-prod",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./lib node ./web/bin/www",
    "docker:build": "docker build -t d4js-2-1 .",
    "docker:build:dev": "docker build -t d4js-2-1:dev -f dev.dockerfile .",
    "docker:run": "npm run docker:stop; docker run --rm -p 3000:3000 --link mongo-apple --name d4js-2-1 d4js-2-1",
    "docker:run:dev": "npm run docker:stop:dev; docker run --rm -p 3000:3000 --link mongo-apple --name d4js-2-1-dev d4js-2-1:dev",
    "docker:start": "docker start d4js-2-1",
    "docker:start:dev": "docker start d4js-2-1-dev",
    "docker:stop:any": "(($(docker ps -f name=$NAME | wc -l) > 1)) && docker stop $NAME || true",
    "docker:stop": "NAME=d4js-2-1 npm run docker:stop:any",
    "docker:stop:dev": "NAME=d4js-2-1-dev npm run docker:stop:any",
    "docker:run:shell": "docker exec -it --user root d4js-2-1 /bin/bash",
    "docker:run:shell:dev": "docker exec -it --user root d4js-2-1-dev /bin/bash"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "epa": "0.2.1",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mongoose": "4.13.2",
    "morgan": "~1.6.1",
    "nanit": "0.3.1",
    "serve-favicon": "~2.3.0"
  }
}
