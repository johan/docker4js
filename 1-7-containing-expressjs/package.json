{
  "name": "1-7-containing-expressjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./lib:$NODE_PATH node ./web/bin/www",
    "docker:build": "docker build -t d4js-1-7 .",
    "docker:debug": "docker run --rm -it d4js-1-7 /bin/bash",
    "docker:run": "r() { npm run docker:stop ; docker run -d --rm --name d4js-1-7 -p 3000:3000 d4js-1-7; }; r",
    "docker:daemon": "npm run docker:run -d",
    "docker:stop": "docker stop d4js-1-7",
    "docker:rm": "docker rm d4js-1-7"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "epa": "0.2.1",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mongoose": "4.13.2",
    "morgan": "~1.6.1",
    "nanit": "0.3.1",
    "serve-favicon": "~2.3.0"
  }
}
